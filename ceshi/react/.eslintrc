{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true
  },
  "rules": {
    "max-len": [
      1,
      140,
      2,
      {
        "ignoreComments": true
      }
    ],
     "linebreak-style": [
      "error",
      "windows"
    ]
  }
}